<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FF736491"
    tools:context=".clublist">
  <View
      android:id="@+id/top_view"
      android:layout_width="match_parent"
      android:layout_height="300dp"
      android:background="@drawable/background"
      />
  <TextView
      android:id="@+id/lists"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_marginStart="50dp"
      android:layout_marginTop="5dp"
      android:layout_marginEnd="50dp"
      android:layout_marginBottom="50dp"
      android:drawableBottom="@drawable/img_2"
      android:drawablePadding="5dp"
      android:gravity="center"
      android:text=" CLUBS LISTS"
      android:textColor="@color/white"
      android:textSize="35dp"
      android:textStyle="bold" />

  <TextView
      android:id="@+id/fac"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_marginStart="10dp"
      android:layout_marginTop="340dp"
      android:layout_marginEnd="10dp"
      android:layout_marginBottom="10dp"
      android:background="#FF8046E7"
      android:gravity="left"
      android:padding="20dp"
      android:hint="fac club"
      android:textStyle="bold"
      android:textSize="20dp"
      android:textColor="@color/black"
      android:textColorHint="@color/black"
      android:drawableLeft="@drawable/grp"
      android:drawablePadding="20dp"
      />
  <TextView
      android:id="@+id/phoratz"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_marginStart="10dp"
      android:layout_marginTop="410dp"
      android:layout_marginEnd="10dp"
      android:layout_marginBottom="10dp"
      android:background="#FF8046E7"
      android:gravity="left"
      android:padding="20dp"
      android:hint="Phoratz club"
      android:textStyle="bold"
      android:textSize="20dp"
      android:textColor="@color/black"
      android:textColorHint="@color/black"
      android:drawableLeft="@drawable/grp"
      android:drawablePadding="20dp"
      tools:ignore="DuplicateIds" />
  <TextView
      android:id="@+id/tamil"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_marginStart="10dp"
      android:layout_marginTop="480dp"
      android:layout_marginEnd="10dp"
      android:layout_marginBottom="10dp"
      android:background="#FF8046E7"
      android:gravity="left"
      android:padding="20dp"
      android:hint="Tamilmandram club"
      android:textStyle="bold"
      android:textSize="20dp"
      android:textColor="@color/black"
      android:textColorHint="@color/black"
      android:drawableLeft="@drawable/grp"
      android:drawablePadding="20dp"
      />
  <TextView
      android:id="@+id/quiz"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_marginStart="10dp"
      android:layout_marginTop="550dp"
      android:layout_marginEnd="10dp"
      android:layout_marginBottom="10dp"
      android:background="#FF8046E7"
      android:gravity="left"
      android:padding="20dp"
      android:hint="quiz club"
      android:textStyle="bold"
      android:textSize="20dp"
      android:textColor="@color/black"
      android:textColorHint="@color/black"
      android:drawableLeft="@drawable/grp"
      android:drawablePadding="20dp"
      />
  <TextView
      android:id="@+id/ncc"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_marginStart="10dp"
      android:layout_marginTop="620dp"
      android:layout_marginEnd="10dp"
      android:layout_marginBottom="10dp"
      android:background="#FF8046E7"
      android:gravity="left"
      android:padding="20dp"
      android:hint="NCC club"
      android:textStyle="bold"
      android:textSize="20dp"
      android:textColor="@color/black"
      android:textColorHint="@color/black"
      android:drawableLeft="@drawable/grp"
      android:drawablePadding="20dp"
      />
  <TextView
      android:id="@+id/red"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_marginStart="10dp"
      android:layout_marginTop="690dp"
      android:layout_marginEnd="10dp"
      android:layout_marginBottom="10dp"
      android:background="#FF8046E7"
      android:gravity="left"
      android:padding="20dp"
      android:hint="Red Ribbon club"
      android:textStyle="bold"
      android:textSize="20dp"
      android:textColor="@color/black"
      android:textColorHint="@color/black"
      android:drawableLeft="@drawable/grp"
      android:drawablePadding="20dp"
      tools:ignore="InvalidId" />
  <TextView
      android:id="@+id/nss"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_marginStart="10dp"
      android:layout_marginTop="760dp"
      android:layout_marginEnd="10dp"
      android:layout_marginBottom="10dp"
      android:background="#FF8046E7"
      android:gravity="left"
      android:padding="20dp"
      android:hint="NSS club"
      android:textStyle="bold"
      android:textSize="20dp"
      android:textColor="@color/black"
      android:textColorHint="@color/black"
      android:drawableLeft="@drawable/grp"
      android:drawablePadding="20dp"
      />



</RelativeLayout>